{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "Components/*": ["./src/components/*"],
      "Contexts/*": ["./src/contexts/*"],
      "Hooks/*": ["./src/hooks/*"],
      "Providers/*": ["./src/providers/*"],
      "Pages/*": ["./src/pages/*"],
      "Public/*": ["./public/*"],
      "Root/*": ["./src/*"]
    }
  },
  "include": ["src", "public"],
  "exclude": ["node_modules", "out"]
}
